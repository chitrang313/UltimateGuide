@model IEnumerable<string>
@inject ICityServices cityServices
@{
  ViewBag.Title = "Home Page";
  List<string> cities = cityServices.GetCities();
}

<h1>Cities</h1>
<ul class="list">
  @foreach (string city in cities) {
    <li>@city</li>
  }
</ul>

<div>@ViewBag.InstanceIdCityServices1</div>
<div>@ViewBag.InstanceIdCityServices2</div>
<div>@ViewBag.InstanceIdCityServices3</div>

<div class="box">@cityServices.ServiceInstanceId</div>
