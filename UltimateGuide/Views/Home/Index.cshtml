@using UltimateGuide.Models

@{
  string title = "asp.net core ultimate guid";
  Person person = new Person {
        Name = "Chitrang",
        DateofBirth = Convert.ToDateTime("1994-10-18"),
      };
}
<!DOCTYPE html>
<html>
<head>
  <title>@title</title>
  <meta charset="utf-8" />
</head>
<body>
  <h1>Welcome to the View</h1>
  <h2>@person.Name</h2>
  @{
    double age = MathF.Round((float)((DateTime.Now - person.DateofBirth).TotalDays / 365.25));
  }
  <h3>@age year old</h3>
</body>
</html>