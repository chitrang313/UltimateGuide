@using UltimateGuide.Models
<div class="box">
  <h3>@ViewBag.Grid.GridTitle</h3>
  <table class="table w-100">
    <thead>
      <tr>
        <th>Sl. No</th>
        <th>Name</th>
      </tr>
    </thead>
    <tbody>
      @foreach (Person person in ViewBag.Grid.Persons) {
        <tr>
          <td>@person.PersonName</td>
          <td>@person.PersonJobTitle</td>
        </tr>
      }
    </tbody>
  </table>
</div>
